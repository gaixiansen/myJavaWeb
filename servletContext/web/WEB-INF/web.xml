<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>servletcontextdemo1</servlet-name>
        <servlet-class>com.github.ServletContextDemo1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletcontextdemo2</servlet-name>
        <servlet-class>com.github.ServletContextDemo2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletcontextdemo1</servlet-name>
        <url-pattern>/demo1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletcontextdemo2</servlet-name>
        <url-pattern>demo2</url-pattern>
    </servlet-mapping>
</web-app>